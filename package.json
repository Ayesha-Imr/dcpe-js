{
  "name": "dcpe-js",
  "version": "0.1.1",
  "description": "Distance Comparison Preserving Encryption for secure searchable vector embeddings",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src",
    "prepublishOnly": "npm run clean && npm run build",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "encryption",
    "vector-search",
    "vector-database",
    "searchable-encryption",
    "dcpe",
    "zero-trust",
    "client-side-encryption",
    "rag",
    "embeddings",
    "security",
    "llm",
    "privacy"
  ],
  "author": "Ayesha Imran",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ayesha-Imr/dcpe-js.git"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/node": "^22.14.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.6",
    "rollup-plugin-polyfill-node": "^0.13.0"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "mathjs": "^14.4.0"
  }
}
